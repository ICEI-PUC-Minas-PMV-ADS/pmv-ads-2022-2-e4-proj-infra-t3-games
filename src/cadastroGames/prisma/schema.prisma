generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Games {
  game_id    String @id @default(auto()) @map("_id") @db.ObjectId
  nome       String
  descricao  String
  url_imagem String
  genero     String
  quantidade Int
}
